{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "articles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      }
    },
    "articles": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": false,
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
